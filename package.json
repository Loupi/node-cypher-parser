{
  "name": "cypher-parser",
  "version": "0.1.0",
  "description": "A Node addon on top of libcypher-parser, used to parse and lint cypher queries.",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build-ts": "tsc",
    "tslint": "tslint -c tslint.json -p tsconfig.json",
    "build": "npm run build-ts && npm run tslint",
    "prepublish": "npm run build",
    "typedoc": "typedoc --mode modules -module commonjs --out docs lib/"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Loupi/node-cypher-parser.git"
  },
  "keywords": [
    "node",
    "cypher",
    "graph",
    "database",
    "query",
    "language",
    "parser",
    "linter",
    "vertex",
    "relation",
    "edge",
    "addon"
  ],
  "author": "Louis-Pierre Beaumont",
  "license": "Apache-2.0",
  "gypfile": true,
  "bugs": {
    "url": "https://github.com/Loupi/node-cypher-parser/issues"
  },
  "homepage": "https://github.com/Loupi/node-cypher-parser#readme",
  "dependencies": {
    "nan": "^2.10.0"
  },
  "devDependencies": {
    "@types/node": "^10.1.3",
    "tslint": "^5.8.0",
    "typedoc": "^0.11.1",
    "typescript": "^2.8.3"
  },
  "config": {
    "unsafe-perm": true
  }
}
