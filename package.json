{
  "name": "cypher-parser",
  "version": "0.1.0",
  "description": "A Node addon on top of libcypher-parser, used to parse and lint cypher queries.",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build-ts": "tsc",
    "tslint": "tslint -c tslint.json -p tsconfig.json",
    "build": "npm run build-ts && npm run tslint",
    "prepublish": "npm run build",
    "typedoc": "typedoc --mode modules -module commonjs --out docs lib/",
    "install": "node-pre-gyp install --fallback-to-build",
    "prepublishOnly": "npm ls",
    "test": "./node_modules/.bin/mocha --compilers ts:ts-node/register ./test/*.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Loupi/node-cypher-parser.git"
  },
  "keywords": [
    "node",
    "cypher",
    "graph",
    "database",
    "query",
    "language",
    "parser",
    "linter",
    "vertex",
    "relation",
    "edge",
    "addon"
  ],
  "author": "Louis-Pierre Beaumont",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/Loupi/node-cypher-parser/issues"
  },
  "homepage": "https://github.com/Loupi/node-cypher-parser#readme",
  "dependencies": {
    "nan": "^2.10.0",
    "node-pre-gyp": "^0.10.0"
  },
  "devDependencies": {
    "@types/chai": "^4.1.3",
    "@types/mocha": "^5.2.1",
    "@types/node": "^10.3.2",
    "aws-sdk": "^2.255.1",
    "chai": "^4.1.2",
    "mocha": "^5.2.0",
    "ts-node": "^6.1.0",
    "tslint": "^5.8.0",
    "typedoc": "^0.11.1",
    "typescript": "^2.9.1"
  },
  "config": {
    "unsafe-perm": true
  },
  "bundledDependencies": [
    "node-pre-gyp"
  ],
  "binary": {
    "module_name": "cypher",
    "module_path": "./dist/release/",
    "host": "https://cypher-parser.s3.amazonaws.com/"
  }
}
